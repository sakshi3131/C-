#include <iostream>

using namespace std;

int main()
{
    int n,flag;
    std::cout << "Enter num :" << std::endl;
    std::cin >> n;
    for(int i=2;i<=n/2;i++){
        flag=0;
        if(n%i==0){
            flag =1;
            break;
        }
    }
    if(flag==0){
        cout<<"is a prime :"<<n;
    }
    else{
        cout<<"NOT A "<<n;
    }
    return 0;
}
